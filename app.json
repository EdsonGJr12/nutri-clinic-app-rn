{
  "expo": {
    "name": "nutri-clinic-app",
    "slug": "nutri-clinic-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F9FA"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.nutriclinicapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-adaptive-icon.png",
        "backgroundColor": "#F8F9FA"
      },
      "package": "br.com.nutriclinicapp",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo necessita acessar suas fotos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4104eb9f-7286-44d2-9b65-6f12054f25c4"
      }
    }
  }
}
